# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'DITest' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DITest
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'Swinject'

  target 'DITestTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
  end

  target 'DITestUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
